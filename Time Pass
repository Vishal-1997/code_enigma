#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
ll t, x, y, a, b, n, cnt;
int main () {
	scanf ("%lld", &t);
	while (t--) {
		scanf ("%lld", &n);
		cnt = 0;
		map < ll, ll > mp_x, mp_y;
		map < pair < ll, ll >, ll > q;
		for (int i = 0; i < n; ++i) {
			scanf ("%lld%lld", &x, &y);
			cnt += mp_x[x] + mp_y[y];
			mp_x[x]++;
			mp_y[y]++;
			cnt -= q[make_pair (x, y)]++;
		}
		printf("%lld\n", cnt);
	}
	return 0;
}
